{
    "name": "@trojs/deep-merge",
    "description": "Object deep merge",
    "version": "0.4.3",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://trojs.org/"
    },
    "license": "MIT",
    "scripts": {
        "lint": "eslint src/*.js --config .eslintrc",
        "lint:report": "eslint src/*.js --config .eslintrc -f json -o report.json",
        "lint:fix": "eslint src/*.js --config .eslintrc --fix",
        "lint:rules": "eslint --print-config file.js > eslintconfig.json",
        "test": "c8 node --test src/*.test.js",
        "test:watch": "c8 node --watch --test src/*.test.js",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --omit=dev"
    },
    "type": "module",
    "files": [
        "src/index.js",
        "src/deep-merge.js",
        "src/schema.d.ts"
    ],
    "main": "src/index.js",
    "types": "src/schema.d.ts",
    "devDependencies": {
        "@hckrnews/eslint-config": "^3.0.0",
        "c8": "^10.0.0",
        "eslint": "^8.23.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^48.0.0",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.0.1",
        "eslint-plugin-sonarjs": "^0.25.0",
        "jscpd": "^4.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/trojs/deep-merge"
    },
    "engines": {
        "node": ">= 18.13"
    },
    "keywords": [
        "object",
        "merge",
        "deep"
    ],
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/w3nl"
    }
